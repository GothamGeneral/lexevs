<?xml version="1.0"?>
<binding xmlns="http://www.castor.org/SourceGenerator/Binding" 
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
            xsi:schemaLocation="http://www.castor.org/SourceGenerator/Binding
                http://castor.org/binding.xsd" defaultBindingType="type">

		<attributeBinding name="/complexType:Directory/@prev">
			<member name="prev" handler="org.cts2.castor.fieldhandler.DirectoryURIFieldHandler"/>
		</attributeBinding>

 		<attributeBinding name="/complexType:Directory/@next">
			<member name="next" handler="org.cts2.castor.fieldhandler.DirectoryURIFieldHandler"/>
		</attributeBinding>
		
		<attributeBinding name="/complexType:CodeSystemVersion/entityDescriptions">
			<member name="entityDescriptions" java-type="org.cts2.uri.EntityDirectoryURI" handler="org.cts2.castor.fieldhandler.DirectoryURIFieldHandler"/>
		</attributeBinding>
		
		<attributeBinding name="/complexType:CodeSystemVersion/associations">
			<member name="associations" java-type="org.cts2.uri.AssociationDirectoryURI" handler="org.cts2.castor.fieldhandler.DirectoryURIFieldHandler"/>
		</attributeBinding>

</binding>