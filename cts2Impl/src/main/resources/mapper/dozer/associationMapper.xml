		<field>
			<a>entity.entityCode</a>
			<b>subject.localEntityName.name</b>
		</field>
		<field>
			<a>entity.entityCodeNamespace</a>
			<b>subject.localEntityName.namespace</b>
		</field>
	</mapping>
