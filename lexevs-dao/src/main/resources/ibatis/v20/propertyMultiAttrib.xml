	<insert id="insertPropertyMultiAttrib" parameterClass="org.lexevs.dao.database.ibatis.property.parameter.InsertPropertyMultiAttribBean">
		INSERT INTO $prefix$propertyMultiAttrib (
			propMultiAttribGuid,
			propertyGuid,
			attributeType,
			attributeId,
			attributeValue,
			subRef,
			role,
			qualifierType,
			entryStateGuid
		) VALUES (
			#uid#,
			#propertyUId#,
			#attributeType#,
			#attributeId#,
			#attributeValue#,
			#subRef#,
			#role#,
			#qualifierType#,
			#entryStateUId#
		)
	</insert>

</sqlMap>