      		<url name="akka-repos" m2compatible="true">
            	<artifact pattern="http://scalablesolutions.se/akka/repository/se/scalablesolutions/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]"/>
			</url>
	
			<url name="scala-repos" m2compatible="true">
            	<artifact pattern="http://scala-tools.org/repo-releases/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]"/>
			</url>
			-->
			
<!--  			<url name="eaio-repos" m2compatible="true" checksums="">
            	<artifact pattern="http://eaio.com/maven2/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]"/>
			</url> -->
			
			<url name="CBIIT Nexus Server" m2compatible="true" checksums="">
        		<artifact pattern="https://ncimvn.nci.nih.gov/nexus/content/groups/public/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]"/>
     		</url>
			
		</chain>
	</resolvers>
</ivysettings>
