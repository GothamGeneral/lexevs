<?xml version="1.0" encoding="utf-8"?>
<!-- JNLP File for the LexGrid Converter -->
<jnlp
  spec="1.0+"
  codebase="http://mir04.mayo.edu/LexGrid/downloads/Conversion%20Tools/LexGrid%20Converter/webstart/"
  href="converter.jnlp">
  <information>
    <title>LexGrid Converter</title>
    <vendor>Biomedical Informatics</vendor>
    <homepage href="http://mir04.mayo.edu/LexGrid/index.php?page=convert"/>
    <description>A Converter program for LexGrid formats</description>
    <icon href="convert.gif"/>
    <offline-allowed/>
  </information>
  <security>
      <all-permissions/>
  </security>
  <resources>
