    <!-- http://purl.bioontology.org/ontology/npo#NPO_160 -->

    <owl:Class rdf:about="#NPO_160">
        <rdfs:label rdf:datatype="&xsd;string"
            >cadmium selenide quantum dot</rdfs:label>
        <rdfs:subClassOf rdf:resource="#NPO_589"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_component_part"/>
                <owl:someValuesFrom rdf:resource="#NPO_228"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <definition rdf:datatype="&rdf;XMLLiteral"
            >&lt;ncicp:ComplexDefinition&gt;&lt;ncicp:def-definition&gt;A quantum dot composed of cadmium selenide.&lt;/ncicp:def-definition&gt;&lt;/ncicp:ComplexDefinition&gt;</definition>
        <synonym rdf:datatype="&xsd;string"
            >CdSe quantum dot</synonym>
        <code rdf:datatype="&xsd;string">NPO_160</code>
        <preferred_Name rdf:datatype="&xsd;string"
            >cadmium selenide quantum dot</preferred_Name>
    </owl:Class>

