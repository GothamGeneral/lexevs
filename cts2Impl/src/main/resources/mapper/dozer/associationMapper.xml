		<class-a>org.LexGrid.LexBIG.DataModel.Core.AssociatedConcept</class-a>
		<class-b>org.cts2.association.Association</class-b>
		<field>
			<a>referencedEntry.entityCode</a>
			<b>target.entity.localEntityName.name</b>
		</field>
		<field>
			<a>referencedEntry.entityCodeNamespace</a>
			<b>target.entity.localEntityName.namespace</b>
		</field>
	</mapping>

	<mapping>
