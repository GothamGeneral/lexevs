	<jar href="lgConverter.jar"/>
    <jar href="apache-commons.jar"/>
    <jar href="eclipse-core.jar"/>
    <jar href="eclipse-emf.jar"/>
    <jar href="lgServicesIndex.jar"/>
    <jar href="lucene.jar"/>
    <jar href="otherJars.jar"/>
    <jar href="protege.jar"/>
    <jar href="sqlDrivers.jar"/>
  </resources>
  <resources os="Windows">
    <nativelib href="swt-windowsNatives.jar"/> 
    <jar href="swt-windows.jar"/>
   </resources>
  <resources os="Linux" arch="x86">
    <nativelib href="swt-linuxNatives.jar"/> 
    <jar href="swt-linux.jar"/>
  </resources>
    <resources os="Linux" arch="i386">
    <nativelib href="swt-linuxNatives.jar"/> 
    <jar href="swt-linux.jar"/>
  </resources>
    <resources os="Linux" arch="i686">
    <nativelib href="swt-linuxNatives.jar"/> 
    <jar href="swt-linux.jar"/>
  </resources>
  <resources os="Linux" arch="amd64">
    <nativelib href="swt-linuxNatives_x86_64.jar"/> 
    <jar href="swt-linux_x86_64.jar"/>
  </resources>
  <application-desc main-class="edu.mayo.informatics.lexgrid.convert.swt.Converter"/>
</jnlp> 